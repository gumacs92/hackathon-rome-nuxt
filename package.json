{
  "name": "rome-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest",
    "generate-types": "npx typechain --target=truffle-v5 'build/contracts/*.json'",
    "postinstall": "npx truffle compile && npm run generate-types",
    "deploy-contracts": "rm -rf ./build/contracts/* && truffle migrate --reset --network development && npm run generate-types"
  },
  "dependencies": {
    "@apollo/client": "^3.6.5",
    "@nuxtjs/axios": "^5.13.6",
    "bootstrap": "^5.1.3",
    "core-js": "^3.15.1",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.8",
    "fs": "^0.0.1-security",
    "graphql": "^16.5.0",
    "js-cookie": "^3.0.1",
    "moralis": "^1.8.0",
    "nuxt": "^2.15.7",
    "react": "^18.1.0",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.2.0",
    "@typechain/truffle-v5": "^8.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.40",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^27.0.5",
    "omit-deep": "^0.3.0",
    "postcss": "^8.3.5",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "truffle": "^5.5.17",
    "ts-node": "^10.8.1",
    "typechain": "^8.0.0",
    "typescript": "^4.7.3",
    "vue-jest": "^3.0.4"
  }
}
